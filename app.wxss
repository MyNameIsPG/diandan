/* 溢出隐藏 一行 */
.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/* 溢出隐藏 二行 */
.ellipsis-2 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
/* 溢出隐藏 二行 */
.ellipsis-5 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  -webkit-box-orient: vertical;
}
.swiper {
  margin-top: 0px;
  height: 345rpx;
  width: 701rpx;
}

.swiper image {
  height: 100%;
  width: 100%;
}

.swiper  .wx-swiper-dot {
  display: inline-flex;
  justify-content: space-between;
  border-radius: 50%;
  margin-bottom: 20px;
}

.swiper .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: #fff;
  border-radius: 50%;
}

.swiper .wx-swiper-dot-active::before {
  background: #d74a45;
  border-radius: 50%;
}

