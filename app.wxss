.prizeList_bottom>view{width: 25%;font-size: 12px;height: 35px;text-align: center;padding: 0;line-height: 35px;}
/* 组团开黑 */
.yuying-group-item{position: relative;width: 710rpx;height: 350rpx;margin: 0 auto;margin-bottom: 20rpx;overflow: hidden}
.yuying-group-item-content{width: 570rpx;height: 210rpx;padding: 70rpx; position: absolute;top: 0;left: 0;z-index: 10;text-align: center}
.yuying-group-item-title{font-size: 36rpx;color: #ffffff;font-weight: bold}
.yuying-group-item-rlue{color: rgba(255, 255, 255,0.8);font-size: 24rpx;margin-top: 30rpx;margin-bottom: 50rpx;line-height: 1.5}
.yuying-group-item-time{color: rgba(255, 255, 255,0.8);font-size: 24rpx;}
/* 创建团队 */
.yuying-createTeam{position: relative}
.yuying-createTeam-content{position: absolute;top: 360rpx;left: 0;z-index: 10;padding: 70rpx 40rpx;width: calc(100% - 80rpx);color:rgba(255, 255, 255,0.8); }
.yuying-createTeam-content .clearfix{margin-bottom: 40rpx}
.yuying-team-property{font-size: 30rpx;width: 150rpx;color: rgba(255, 255, 255,0.6);}
.yuying-team-type{font-size: 32rpx;width: calc(100% - 150rpx);font-weight: bold;color: #fff}
.yuying-team-reward{font-size: 30rpx;color: #fff;width: calc(100% - 150rpx)}
.yuying-team-time{font-size: 30rpx;color: #fff;width: calc(100% - 150rpx)}
.yuying-createTeam-content .yuying-team-list{margin-bottom: 30rpx;margin-left: 30rpx;}
.yuying-createTeam-content input{width: calc(100% - 42rpx);height: 80rpx;border:1rpx solid rgba(255, 255, 255,0.3);margin-top: 70rpx;
padding-left: 20rpx;padding-right: 20rpx;}
.yuying-createTeam-btn{height:100rpx;width: 100%;background: #13131b;line-height: 100rpx;text-align: center;color:#fff;font-size: 32rpx;position: fixed;left: 0;bottom: 0px;z-index: 11;border-radius:0px;}
.yuying-createTeam-btn button{height:100rpx;line-height:100rpx;font-size:36rpx;}
.yuying-team-name,.yuying-team-slogan{font-size: 36rpx;color: #ffa061;text-align: center;width: 100%;margin-top: 20rpx;}
.yuying-team-name-bg{background: rgba(238, 238, 238, 0.1);padding-top: 30rpx;padding-bottom: 30rpx;margin-top: 30rpx;border-radius:10rpx;}
.yuying-team-name-bg .yuying-team-list{margin-top: 30rpx;margin-bottom: 0}
.yuying-team-name-bg .yuying-team-list:first-child{margin-top: 0;}
/* 分享组团开黑 */
.yuying-createTeam-activityTtime{font-size: 30rpx;text-align: center;color: #fff;margin-top: 50rpx;margin-bottom: 50rpx;}
.yuying-groupTeam-rule{width: calc(100% - 2rpx);border: 1rpx solid rgba(255, 255, 255,0.3);min-height:150rpx;position: relative ;}
.yuying-groupTeam-title{width:161rpx;height: 59rpx;position: absolute;top: -59rpx;left: 50%;margin-left: -80rpx;z-index: 20;
  }
  .yuying-groupTeam-teammate{width: 100%;text-align: center;margin-top: 80rpx;}
  .yuying-groupTeam-teammate text{padding: 2rpx 5rpx;border:1px solid rgba(255, 255, 255,0.3);display: inline-block;margin-left: 5rpx;margin-right: 5rpx;}
.yuying-teammate-item{width: 100rpx;height: 100rpx;border:2rpx solid rgba(255, 255, 255,0.3);margin-right: 7rpx;margin-bottom: 7rpx;}
.yuying-groupTeam-countDown{text-align: center;margin-top: 60rpx;}
.yuying-groupTeam-datetime{width: 350rpx;margin: 15rpx auto;}
.yuying-groupTeam-datetime .pull-left{border:1px solid rgba(255, 255, 255,0.3);width: 46rpx;}
.yuying-groupTeam-datetime .no-border{border: none} 
.yuying-groupTeam-bnt{width: 314rpx;height: 83rpx;position: relative}
.yuying-groupTeam-bnt>view{width: 314rpx;height: 83rpx;text-align: center;line-height: 83rpx;position: absolute;top: 0;left: 0;z-index: 10;font-weight: bold;font-size: 32rpx}
/* 我的抱团 */
.yuying-hugs_list-icon{width:11rpx;height:46rpx;display:block;position:absolute;top:20rpx;left:0;z-index:1;border-radius:11rpx;
}
.hugs-doing-color .yuying-hugs_list-icon {background:#d74a45 }
.hugs-quit-color .yuying-hugs_list-icon{background:#ff0b50 }
.hugs-success-color .yuying-hugs_list-icon{background:#01c245 }
.hugs-revoke-color .yuying-hugs_list-icon{background:#ccc }
.hugs-doing-color .yuying-hugs-state {color:#d74a45 }
.hugs-quit-color .yuying-hugs-state{color:#ff0b50 }
.hugs-success-color .yuying-hugs-state{color:#01c245 }
.hugs-revoke-color .yuying-hugs-state{color:#ccc }
.yuying-hugs-list{margin-bottom:45rpx;background:#fff;border-radius:10rpx;position:relative;padding:20rpx;overflow:hidden;
box-shadow:0px 0px 23px 0px rgba(215, 74, 69, 0.2);padding-top: 24rpx;}
.yuying-hugs-member{margin-top: 20rpx;margin-bottom: 20rpx;padding-top: 20rpx;padding-bottom: 20rpx;border-bottom: 1rpx solid #eee;border-top: 1rpx solid #eee}
.yuying-hugs-member image{width: 80rpx;height: 80rpx;display: inline-block;margin-right: 10rpx;border:1rpx solid #999}
.yuying-hugs-mendian,.yuying-hugs-type{font-size: 32rpx;font-weight: bold;color: #2b2b2b}
.yuying-hugs-job{font-size: 30rpx;color: #2b2b2b;margin-left: 75rpx;}
.yuying-hugs-dateTime{font-size: 24rpx;color: #666;}
.yuying-hugs-state{font-size: 24rpx;padding: 2rpx 8rpx}
/* 抱团详情 */
.yuying-hugsDetail-content{background:#fff;border-radius:10rpx;position:relative;padding:20rpx;
box-shadow:0px 0px 23px 0px rgba(215, 74, 69, 0.2);padding-bottom: 80rpx;}
.yuying-hugsDetail-item{height: 85rpx;border-bottom: 1rpx solid #eee}
.yuying-hugsDetail-item>view{height: 85rpx;line-height: 85rpx;font-size: 30rpx;}
.yuying-hugsDetail-item>view.pull-right{font-weight: bold}
.yuying-hugsDetail-item-member{height: 85rpx;line-height: 85rpx;}
.yuying-hugsDetail-member image{width: 80rpx;height: 80rpx;display: inline-block;margin-right: 10rpx;border:1rpx solid #999}
.yuying-hugs-quit-btn{height: 80rpx;line-height: 80rpx;text-align: center;font-size: 30rpx;background: #d74a45;
border-radius: 10rpx;color: #fff;box-shadow:0px 0px 23px 0px rgba(215, 74, 69, 0.2);margin: 10rpx }
.yuying-createTeam-mendian-name{font-size:36rpx;font-weight:bold;color:#fff;text-align: center}
.no-detail{width: 100%;}
.no-detail image{width: 300rpx;height: 300rpx;display: block;margin: 0 auto; margin-top:100rpx }
.no-detail-text{font-size: 30rpx;color: #999;text-align: center}
/* 关于我们 */
.about_us{padding:80rpx 50rpx;text-align:left;font-size: 32rpx;color: #333}
.about_us view{line-height: 1.8;width: 100%;margin-top: 30rpx}
.text-right{text-align: right}

/* 签到 */
.yuying-sign-header-box{background: #d74a45;height: 540rpx;position: relative;color: #fff;}
.yuying-sign-header-bg{width: 742rpx;height: 237rpx;position: absolute;left: 0;top: 80rpx;z-index: 1}
.yuying-sign-header{padding:30rpx;}
.yuying-sign-mendian-name{font-size: 32rpx;font-weight: bold}
.yuying-sign-mendian-score{font-size: 28rpx;}
.yuying-sign-btn{width: 237rpx;height: 238rpx;margin: 0 auto;position: relative;margin-bottom: 10rpx}
.yuying-sign-btn-text{position: absolute;top: 0;left: 0;z-index: 99;line-height: 238rpx;font-size:32rpx;font-weight: bold;width: 237rpx;height: 238rpx;text-align: center}
.yuying-sign-date{height: 90rpx;text-align: center;font-size:34rpx;color: #111;line-height: 90rpx;width: 90%;border-bottom: 1px solid #eee;margin: 0 auto}
.yuying-sign_toast-box{position: relative;width: 100%;height: 100%;background: transparent}
.yuying-sign_toast-box image{position: absolute;top: 0;left: 0;z-index: -1}




